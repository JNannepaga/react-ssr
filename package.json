{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "This is server side rendering app that uses ExpressJs, React, Webpack.",
  "main": "./src/server/index.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "dev:host-server": "nodemon --watch dist --exec \"node dist/server-bundle.js \"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GunduMoni/react-ssr.git"
  },
  "keywords": [
    "Server",
    "Side",
    "Rendering",
    "SSR",
    "React",
    "SSR"
  ],
  "author": "Joseph Abhilash Nannepaga",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GunduMoni/react-ssr/issues"
  },
  "homepage": "https://github.com/GunduMoni/react-ssr#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "serialize-javascript": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-async-to-generator": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "html-webpack-plugin": "^4.5.1",
    "nodeman": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.4.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2"
  }
